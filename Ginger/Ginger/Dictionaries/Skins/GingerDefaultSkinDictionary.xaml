    <ControlTemplate TargetType="{x:Type ToggleButton}" x:Key="$FlatComboBoxToggleButton" >
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="18" />
            </Grid.ColumnDefinitions>
            <Border 
                  Grid.Column="0"
