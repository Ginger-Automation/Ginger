    <Page x:Class="Ginger.Variables.VariableStringPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="60" d:DesignWidth="300"
	Title="VariableStringPage">

    <Grid>
        <StackPanel Background="{StaticResource $BackgroundColor_LightGray}">
            
            <Label Style="{StaticResource @InputFieldLabelStyle}">Initial String Value:</Label>
            <TextBox x:Name="txtStringValue" Style="{StaticResource @TextBoxStyle}" Margin="0,0,0,10"></TextBox>
        </StackPanel>

    </Grid>
</Page>
